<object id="svgId" class="svgClass" type="image/svg+xml" data="/images/background2.svg"></object>
<div class="title"></div>

<div class="container">

  <div class="results">
    {{#each this}}
      <p><a href="{{link}}"><h4>{{recipeName}}</h4></a></p>
      <img src="{{image}}">
    {{/each}}
  </div>

  <a href="#" class="showSuggestion">ADD A SUGGESTION</a>

  <div class="suggestion">
      <form>
        <input data-option="eat" type="search" placeholder="suggest a dish" class="suggestInput"></input>
        <button type="submit">Submit</button>
      </form>
  </div>


</div>